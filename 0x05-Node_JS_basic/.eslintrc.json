{
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "quotes": ["error", "double"],
      "semi": ["error", "always"]
    }
  }
